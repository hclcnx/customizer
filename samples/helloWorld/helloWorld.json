{
  "name": "Homepage Customization",
  "title": "Introductory Customizer App",
  "description": "Inserts text into IBM Connections Homepage",
  "services": [
    "Customizer"
  ],
  "extensions": [
    {
      "payload": {
        "include-files": [
          "helloWorld/helloWorld.user.js"
        ]
      },
      "name": "Hello World Extension",
      "type": "com.ibm.customizer.ui",
      "path": "homepage"
    }
  ]
}
